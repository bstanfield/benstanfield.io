{
    "version": 2,
    "builds": [
        { "src": "./public/**", "use": "@now/static" }
    ],
    "routes": [
        { "src": "/(.*)", "dest": "public/$1" },
        { "src": "/(.*)", "dest": "/projects/coffee.html" } 
    ],
    "alias": [
        "benstanfield.io", "www.benstanfield.io"
    ]
}